<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	version="3.0">
	
	<display-name>Group2</display-name>
	<welcome-file-list>
		<welcome-file>webpages/login.jsp</welcome-file>
	</welcome-file-list>
	
	<servlet>
		<description></description>
		<display-name>CSRepServlet</display-name>
		<servlet-name>CSRepServlet</servlet-name>
		<servlet-class>queryServlets.CSRepServlet</servlet-class>
	</servlet>
	
	<servlet>
		<description></description>
		<display-name>NMEServlet</display-name>
		<servlet-name>NMEServlet</servlet-name>
		<servlet-class>queryServlets.NMEServlet</servlet-class>
	</servlet>
	
	<servlet>
		<description></description>
		<display-name>SEngServlet</display-name>
		<servlet-name>SEngServlet</servlet-name>
		<servlet-class>queryServlets.SEngServlet</servlet-class>
	</servlet>
	
	 <servlet>
        <servlet-name>ImportServlet</servlet-name>
        <servlet-class>main.ImportServlet</servlet-class>
    </servlet>
    
	<servlet>
		<description></description>
		<display-name>LoginServlet</display-name>
		<servlet-name>LoginServlet</servlet-name>
		<servlet-class>main.LoginServlet</servlet-class>
	</servlet>
	<servlet>
		<description></description>
		<display-name>RegisterServlet</display-name>
		<servlet-name>RegisterServlet</servlet-name>
		<servlet-class>main.RegisterServlet</servlet-class>
	</servlet>
	
	
	<servlet-mapping>
        <servlet-name>ImportServlet</servlet-name>
        <url-pattern>/ImportServlet</url-pattern>
    </servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/LoginServlet</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>RegisterServlet</servlet-name>
		<url-pattern>/RegisterServlet</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>CSRepServlet</servlet-name>
		<url-pattern>/CSRepServlet</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>NMEServlet</servlet-name>
		<url-pattern>/NMEServlet</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>SEngServlet</servlet-name>
		<url-pattern>/SEngServlet</url-pattern>
	</servlet-mapping>
	
	<context-param>
		<description>Location to store uploaded file</description>
		<param-name>ImportFile</param-name>
		<param-value>C:\tmpFinal\</param-value>
	</context-param>
</web-app>
